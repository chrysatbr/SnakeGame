public class Snake {
	int snakeId;
	int headId;
	int tailId;
	
	public Snake() {
		snakeId = 0;
		headId = 0;
		tailId = 0;
	}
	
	public Snake(int s, int h, int t) {
		snakeId = s;
		headId = h;
		tailId = t;
	}
	
	public Snake(Snake s) {
		snakeId = s.snakeId;
		headId = s.headId;
		tailId = s.tailId;
	}
	
	public void setSnakeId(int s) {
		snakeId = s;
	}
	
	public void setHeadId(int h) {
		headId = h;
	}
	
	public void setTailId(int h) {
		tailId = h;
	}
	
	public int getSnakeId() {
		return snakeId;
	}
	
	public int getHeadId() {
		return headId;
	}
	
	public int getTailId() {
		return tailId;
	}
	
}
